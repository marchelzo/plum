let makeCounter = function (f) {

        let c = 1;
        let oldc = nil;

        return function () {
                [c, oldc] = [f(c), c];
                return oldc;
        };
};

let counter = makeCounter(function (c) return c * 3 + 1;);

let result = 42;

print(counter());
print(counter());
print(counter());
print(counter());

let counter2 = makeCounter(function (c) return c + 1;);

print(counter2());
print(counter2());
